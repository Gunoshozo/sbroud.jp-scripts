{
  "name": "sbroud.jp-scripts",
  "version": "0.5.0",
  "scripts": {
    "ng": "ng",
    "start-loc": "gulp minify && ng serve --host=0.0.0.0 --disable-host-check",
    "start": "gulp minify && ng serve --configuration production",
    "build": "gulp minify && ng build --configuration production",
    "build-gh": "gulp minify && ng build --configuration production --base-href \"https://gunoshozo.github.io/sbroud.jp-scripts/\"",
    "publish": "npx angular-cli-ghpages --dir=dist/sbroud.jp-scripts/browser",
    "watch": "ng build --watch --configuration production",
    "test": "ng test",
    "e2e": "ng e2e",
    "deploy": "ng deploy",
    "explore": "ng build --configuration production --source-map && source-map-explorer dist/**/*.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/cdk": "^17.2.2",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@taiga-ui/addon-doc": "3.75.0",
    "@taiga-ui/addon-mobile": "^3.75.0",
    "@taiga-ui/cdk": "^3.75.0",
    "@taiga-ui/core": "^3.75.0",
    "@taiga-ui/icons": "^3.75.0",
    "@taiga-ui/kit": "^3.75.0",
    "@taiga-ui/layout": "3.75.0",
    "@taiga-ui/styles": "^3.75.0",
    "@tinkoff/ng-dompurify": "4.0.0",
    "@tinkoff/ng-polymorpheus": "4.3.0",
    "dompurify": "3.0.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.1",
    "@angular/cli": "^17.2.1",
    "@angular/compiler-cli": "^17.2.0",
    "@types/dompurify": "3.0.5",
    "@types/jasmine": "~5.1.0",
    "angular-cli-ghpages": "2.0.1",
    "gulp": "^5.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-json-minify": "^1.2.3",
    "gulp-svgo": "^2.2.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.3.2"
  },
  "overrides": {
    "vite": "5.1.7"
  }
}
